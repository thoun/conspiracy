{OVERALL_GAME_HEADER}

<div id="score">
    <table>
        <thead>
            <tr>
                <th colspan="6"></th>
            </tr>
        </thead>
        <tbody id="score-table-body">
        </tbody>
    </table>
</div>

<div id="stacks">
    <div id="lord-stacks" class="stacks whiteblock">
        <div id="lord-hidden-pile" class="hidden-pile">
            <div class="button" data-number="1">1</div>
            <div class="button" data-number="2">2</div>
            <div class="button" data-number="3">3</div>
        </div>
        <div id="lord-visible-stocks">
            <div id="lord-visible-stock1"></div>
            <div id="lord-visible-stock2"></div>
            <div id="lord-visible-stock3"></div>
            <div id="lord-visible-stock4"></div>
            <div id="lord-visible-stock5"></div>
        </div>
    </div>
    <div id="lord-pick" class="whiteblock">
        <div id="lord-pick-stock"></div>
    </div>

    <div id="location-stacks" class="stacks whiteblock">
        <div id="location-hidden-pile" class="hidden-pile">
            <div class="button" data-number="1">1</div>
            <div class="button" data-number="2">2</div>
            <div class="button" data-number="3">3</div>
        </div>
        <div id="location-visible-stock"></div>
    </div>
    <div id="location-pick" class="whiteblock">
        <div id="location-pick-stock"></div>
    </div>
</div>

<div id="players-tables">
</div>

{OVERALL_GAME_FOOTER}
